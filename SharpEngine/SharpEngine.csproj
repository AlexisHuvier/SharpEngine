<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>0.9.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Little 2D game engine built on Monogame</Description>
    <PackageProjectUrl />
    <Authors>LavaPower - LycosNovation</Authors>
    <Company>LycosNovation</Company>
    <Copyright>Copyright (c) LavaPower 2021</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageVersion>0.10.0</PackageVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aether.Physics2D" Version="1.6.1" />
    <PackageReference Include="MonoGame.Framework.DesktopGL" Version="3.8.1.303" />
    <PackageReference Include="SpriteFontPlus" Version="0.9.2" />
    <PackageReference Include="TiledLoader" Version="1.0.0" />
  </ItemGroup>

</Project>
